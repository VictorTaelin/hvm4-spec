// SWI with sparse values and MAT extracting fields
@sparse = λ{0:1; 10:2; 100:3; 1000:4; λx.0}
@get = λ{#None:0; λsome.λ{#Some:λx.x}(some)}
@pair = λ{#P:λa.λb.a + b; λx.0}
@main = [
  @sparse(0),
  @sparse(100),
  @sparse(50),
  @get(#None{}),
  @get(#Some{42}),
  @pair(#P{3,7})]
//[1,3,0,0,42,10]
